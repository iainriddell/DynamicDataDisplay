<Window x:Class="CoastlineSampleApp.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        Title="WPF DynamicDataDisplay — Coastline Sample" Icon="{x:Static d3:D3IconHelper.DynamicDataDisplayWhiteIcon}">
    <Grid>
        <d3:ChartPlotter Name="plotter">
            <d3:Coastline Name="coastline" />
        </d3:ChartPlotter>
    </Grid>
</Window>
